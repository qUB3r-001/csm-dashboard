{
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "next/core-web-vitals",
    "prettier"
  ],
  "parser": "@typescript-eslint/parser",
  "plugins": ["import", "prettier"],
  "parserOptions": {
    "project": ["./tsconfig.json"]
  },
  "rules": {
    "no-console": "error",
    "react/jsx-filename-extension": "off",
    "react/jsx-props-no-spreading": "off",
    "prettier/prettier": "error"
  }
}
